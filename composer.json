{
    "name": "r-shief/nab3a",
    "description": "Streaming API client",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Doherty",
            "email": "bjd@bangpound.org",
            "homepage": "https://bangpound.org",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nab3aBundle\\": "src/Nab3aBundle/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/ContainerBuilderKernel.php"
        ],
        "files": [
            "src/functions.php",
            "src/Nab3aBundle/Process/functions.php"
        ],
        "exclude-from-classmap": [
            "/Tests/",
            "/test/",
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nab3aBundle\\Tests\\": "tests/Nab3aBundle/"
        }
    },
    "require": {
        "clue/json-stream": "^0.1",
        "evenement/evenement": "^2.0",
        "google/apiclient": "^2.0@rc",
        "guzzlehttp/guzzle": "^6.1",
        "guzzlehttp/oauth-subscriber": "^0.3",
        "igorw/get-in": "^1.0",
        "league/pipeline": "^0.2",
        "matthiasnoback/symfony-bundle-plugins": "^1.1",
        "mtdowling/transducers": "^0.3",
        "react/child-process": "dev-develop as 0.4",
        "react/eep": "dev-develop as 0.1",
        "react/event-loop": "^0.4",
        "react/http": "^0.4",
        "react/stream": "^0.4",
        "symfony/console": "^3.0",
        "symfony/expression-language": "^3.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/intl": "^3.0",
        "symfony/monolog-bundle": "^2.8",
        "symfony/validator": "^3.0",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^1.2",
        "phpunit/phpunit": "^4.8|^5.2",
        "sensio/generator-bundle": "^3.0",
        "symfony/debug-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "wyrihaximus/react-event-loop-inspector": "dev-master"
    },
    "replace": {
        "symfony/asset": "~2.8|~3.0",
        "symfony/class-loader": "~2.8|~3.0",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/polyfill-mbstring": "~1.0",
        "symfony/finder": "~2.8|~3.0",
        "symfony/routing": "~3.0",
        "symfony/security-core": "~2.8|~3.0",
        "symfony/security-csrf": "~2.8|~3.0",
        "symfony/stopwatch": "~2.8|~3.0",
        "symfony/templating": "~2.8|~3.0",
        "symfony/twig-bridge": "~2.8|~3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bangpound/child-process.git"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
